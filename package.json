    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.1.0",
    "jest-chain": "^1.1.5",
    "jest-extended": "^0.11.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "yargs": "^15.1.0"

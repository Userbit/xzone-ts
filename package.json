    "format": "npx prettier --write './**/*.{js,jsx,ts,tsx,json,md}'",
    "format-check": "npx prettier --list-different './**/*.{js,jsx,ts,tsx,json,md}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Userbit/xzone-ts.git"
